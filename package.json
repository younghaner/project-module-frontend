{
  "name": "project-module",
  "version": "1.0.0",
  "main": "src/index.js",
  "scripts": {
    "start": "nodemon -x babel-node src/index.js"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "8.0.0"
          }
        }
      ]
    ]
  },
  "dependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.9",
    "@babel/core": "^7.22.9",
    "@babel/node": "^7.22.6",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-react": "^7.22.5",
    "babel-loader": "^9.1.3",
    "nodemon": "^3.0.1",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "nodemonConfig": {
    "env": {
      "NODE_ENV": "dev"
    },
    "execMap": {
      "js": "node --harmony"
    },
    "ext": "js json",
    "ignore": [
      "dist/"
    ],
    "watch": [
      "src/"
    ]
  }
}
